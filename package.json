{
  "name": "gu-ask-real-ideas",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:style": "yarn stylelint styles **/*.scss",
    "lint:style:fix": "yarn lint:style --fix",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook",
    "test": "jest -w 1",
    "test:u": "jest -w 1 -u",
    "test:cv": "yarn test --coverage",
    "test:cv:msg": "yarn test:cv --verbose",
    "test:cv:w": "yarn test:cv --watchAll",
    "test:cv:msg:w": "yarn test:cv:msg --watchAll",
    "prepare": "husky install",
    "solve:pre:commit:not-exec": "chmod ug+x .husky/*",
    "pre:commit": "yarn lint && yarn build",
    "pre:push": "yarn test:cv:msg"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.5",
    "dotenv": "^16.0.0",
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@types/node": "17.0.16",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "clsx": "^1.1.1",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "postcss": "^8.4.6",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "prettier-stylelint": "^0.4.2",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "stylelint": "^14.5.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^4.1.0",
    "tailwindcss": "^3.0.19",
    "typescript": "4.5.5",
    "webpack": "^5.68.0"
  }
